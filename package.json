{
  "name": "priam",
  "version": "1.2.0",
  "description": "A simple Cassandra driver. It wraps the helenus and cassandra-driver Cassandra drivers with additional error/retry handling, external .cql file support, connection option resolution from an external source, and query composition, among other improvements.",
  "keywords": [
    "cassandra",
    "cql"
  ],
  "license": "MIT",
  "homepage": "https://github.com/godaddy/node-priam",
  "bugs": "http://github.com/godaddy/node-priam/issues",
  "author": {
    "name": "Stephen Commisso",
    "email": "scommisso@godaddy.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/node-priam.git"
  },
  "dependencies": {
    "async": "~0.9.0",
    "cassandra-driver": "~1.0.3",
    "isstream": "~0.1.1",
    "lodash": "~3.3.1",
    "q": "~1.2.0",
    "read-only-stream": "~1.1.1",
    "through2": "~0.6.3",
    "uuid": "~2.0.1"
  },
  "optionalDependencies": {
    "helenus": "~0.6.10"
  },
  "devDependencies": {
    "mocha": "~2.1.0",
    "chai": "~2.1.1",
    "sinon": "~1.12.2",
    "sinon-chai": "^2.7.0",
    "istanbul": "~0.3.6",
    "mocha-istanbul": "~0.2.0",
    "jshint": "~2.6.3",
    "winston": "^0.9.0",
    "coveralls": "^2.11.2",
    "mocha-lcov-reporter": "~0.0.2"
  },
  "scripts": {
    "pretest": "node node_modules/jshint/bin/jshint ./lib/. ./test/. ./example/. ./index.js",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "start": "node example/example"
  }
}
